<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.michaeloles.swiftset">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:versionCode="3"
        android:versionName="1.2.1">
        <activity
            android:name=".MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategorySelector" />
        <activity
            android:name=".ExerciseSelector"
            android:label="@string/title_activity_exercise_selector"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ExerciseViewer" />
        <activity android:name=".WorkoutViewer" />
        <activity android:name=".DemoVideo"></activity>
    </application>

</manifest>
